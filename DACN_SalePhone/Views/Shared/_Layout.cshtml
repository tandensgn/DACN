    <script src="~/Assets/client/js/jquery.flexslider.js"></script>
    <!-- //flexslider js -->
    <!-- flexslider js -->
    <script src="~/Assets/client/js/SmoothScroll.min.js"></script>
    <!-- //flexslider js -->
